      },
      "cordova-sqlite-storage": {},
      "cordova-plugin-file": {
        "ANDROIDX_WEBKIT_VERSION": "1.4.0"
      },
      "cordova-plugin-splashscreen": {}
    }
  },
  "dependencies": {
    "@spoonconsulting/cordova-plugin-local-notification": "^1.0.6"
  }
